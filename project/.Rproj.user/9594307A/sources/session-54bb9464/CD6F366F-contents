#Working With Data
install.packages("tidyverse")
library(ggplot2)
data()

data("diamonds")
View(diamonds)
head(diamonds) #solo los 6 primeros registros
colnames(diamonds)
glimpse(diamonds)
str(diamonds)

library(tidyverse)
mutate(diamonds,carat_2 = carat*100)
View(diamonds)

as_tibble(diamonds) #returns first 10 rows
